extends layout.pug
include utils.pug
block content
    div(class='container')
        div(class='tapes')
            h1(style='text-align: center') Saved songs in your #{tape.tapeName}
            table
                thead
                    tr
                        th(scope='col') Song
                tbody
                    each playlist in playlists
                        tr
                            td= playlist.Song.name